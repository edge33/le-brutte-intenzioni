{
  "manifest_version": 3,
  "name": "Le brutte intenzioni...",
  "description": "Il lorem ipsum dello sviluppatore web fermo al 2020",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.%contentHash%.js"]
